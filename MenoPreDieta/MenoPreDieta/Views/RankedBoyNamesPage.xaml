<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="MenoPreDieta.Views.RankedBoyNamesPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MenoPreDieta.Views;assembly=MenoPreDieta"
    Title="Rebríček chlapčenských mien"
    Shell.BackgroundColor="{DynamicResource Blue}">

    <ContentPage.ToolbarItems>
        <ToolbarItem
            Command="{Binding ResetCommand}"
            IconImageSource="ic_reset.png"
            Text="Začať odznova" />
    </ContentPage.ToolbarItems>

    <ContentPage.Content>
        <views:RankedNamesView />
    </ContentPage.Content>
</ContentPage>